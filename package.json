  },
  "homepage": "https://github.com/crccheck/box-boy#readme",
  "dependencies": {
    "static-kdtree": "^1.0.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.1.8",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.8",
    "browserify": "^14.1.0",
    "concurrently": "^3.3.0",
    "node-sass": "^4.5.0",
    "watchify": "^3.9.0"
  }
